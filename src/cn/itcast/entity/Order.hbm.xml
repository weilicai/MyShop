<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.itcast.entity.Order" table="t_order">
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>
		<property name="oid" column="oid"></property>
		<property name="username" column="username"></property>
		<property name="modelNo" column="modelNo"></property>
		<property name="img" column="img"></property>
		<property name="size" column="size"></property>
		<property name="color" column="color"></property>
		<property name="quantity" column="quantity"></property>
		<property name="amount" column="amount"></property>
		<property name="address" column="address"></property>
		<property name="buyDate" column="buyDate"></property>
	</class>
</hibernate-mapping>





