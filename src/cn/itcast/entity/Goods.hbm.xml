<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.itcast.entity.Goods" table="t_goods">
		<id name="gid" column="gid">
			<generator class="native"></generator>
		</id>
		<property name="modelNo" column="modelNo"></property>
		<property name="name" column="name"></property>
		<property name="color" column="color"></property>
		<property name="size" column="size"></property>
		<property name="price" column="price"></property>
		<property name="img" column="img"></property>
		<property name="inventory" column="inventory"></property>
		<property name="dcb" column="dcb"></property>
		
	</class>
</hibernate-mapping>





